# AAIRE Environment Configuration
# Copy this file to .env and update with your actual values

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-4o-mini  # Override default model

# Vector Database Configuration (choose one)

# Qdrant Cloud (Recommended - better free tier)
# QDRANT_URL=your_qdrant_cluster_url_here
# QDRANT_API_KEY=your_qdrant_api_key_here

# Pinecone (Alternative)
# PINECONE_API_KEY=your_pinecone_api_key_here
# PINECONE_ENVIRONMENT=your_pinecone_environment_here

# Redis Configuration (Optional - for caching)
# REDIS_URL=redis://localhost:6379/0
# REDIS_PASSWORD=your_redis_password_here

# AWS S3 Configuration (Optional - for document storage)
# AWS_ACCESS_KEY_ID=
# AWS_SECRET_ACCESS_KEY=
# AWS_REGION=us-east-1
# S3_BUCKET_NAME=aaire-documents
ALLOWED_ORIGINS=https://aaire.xyz,https://www.aaire.xyz
DOMAIN=aaire.xyz
SSL_ENABLED=true
SECURE_COOKIES=true